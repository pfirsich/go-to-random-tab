{
    "manifest_version": 2,
    "name": "Go To Random Tab",
    "version": "1.0",
    "description": "Provides a button to go to a random tab in the current window.",
    "icons": {
        "512": "icon.png"
    },
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Go To Random Tab"
    },
    "background": {
        "scripts": [
            "go-to-random-tab.js"
        ]
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "go-to-random-tab@joelschumacher.de",
            "strict_min_version": "102.0"
        }
    }
}
